{
  "name": "@hyperwatch/hyperwatch",
  "description": "Open Source HTTP Traffic Manager",
  "license": "Apache-2.0",
  "author": "Access Watch Technologies GmbH <team@access.watch>",
  "contributors": [
    "Benoît Fleury <benoit@access.watch>",
    "François Hodierne <francois@access.watch>",
    "Jean Duthon <jean@access.watch>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/znarf/hyperwatch.git"
  },
  "version": "3.0.2",
  "main": "hyperwatch.js",
  "bin": {
    "hyperwatch": "./bin/hyperwatch"
  },
  "scripts": {
    "depcheck": "depcheck",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "start": "node start",
    "test": "mocha --recursive",
    "prettier": "prettier \"**/*.@(js|json|md|yml)\"",
    "prettier:check": "npm run prettier -- --list-different",
    "prettier:write": "npm run prettier -- --write"
  },
  "dependencies": {
    "ajv": "^6.11.0",
    "date-format-lite": "^17.7.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "fs-extra": "^9.0.0",
    "geoip-lite": "^1.4.0",
    "immutable": "^4.0.0-rc.12",
    "lodash.merge": "^4.6.2",
    "micro-strptime": "^0.2.3",
    "on-finished": "^2.3.0",
    "proxy-addr": "^2.0.5",
    "rc": "^1.2.8",
    "syslog-parse": "^1.3.1",
    "tail": "^2.0.3",
    "uuid": "^8.0.0",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "depcheck": "^0.9.2",
    "eslint": "^7.0.0",
    "eslint-plugin-node": "^11.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^7.0.1",
    "prettier": "^2.0.5",
    "prettier-package-json": "^2.1.3"
  },
  "engines": {
    "node": ">=11.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier-package-json --write"
    ]
  }
}
